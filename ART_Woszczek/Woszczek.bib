@article{bell_bertlmanns_1981,
	title = {Bertlmann's socks and the nature of reality},
	volume = {42},
	issn = {0449-1947},
	url = {http://www.edpsciences.org/10.1051/jphyscol:1981202},
	doi = {10.1051/jphyscol:1981202},
	abstract = {The Einstein-Podolsky-Rosen correlations are very like many ordinary occurrences of everyday life. So it is a little difficult for the man in the street to understand immediately why there has been so much fuss about them. It must be recalled that the founding fathers of quantum mechanics had convinced themselves that it was necessary to abandon the idea of an objective reality at the microphysical level. But the correlations in question, together with the idea of local causality, were a formidable argument for such a reality. The founding fathers offered counter-arguments (neither very clear nor very convincing in my opinion) and each side held to its position. Since then it has been possible to push the analysis a little further, considering especially situations just alittle different from those considered before. Then correlations appear, according to quantum mechanics, which are not at all like those of everyday life. As a result it is not now easy to believe, with Einstein, that quantum mechanical predictions are reconcilable with the notion of a Lorentz invariant objectively real microphysical world.},
	language = {en},
	number = {C2},
	urldate = {2020-11-24},
	journal = {Le Journal de Physique Colloques},
	author = {Bell, John S.},
	month = mar,
	year = {1981},
	pages = {C2--41--C2--62},
	file = {Bell - 1981 - BERTLMANN'S SOCKS AND THE NATURE OF REALITY.pdf:/home/pawel/.zotero/zotero/2felkxqq.default/zotero/storage/2TCRZ7C6/Bell - 1981 - BERTLMANN'S SOCKS AND THE NATURE OF REALITY.pdf:application/pdf;Przesłana wersja:/home/pawel/.zotero/zotero/2felkxqq.default/zotero/storage/VBMCQTEG/Bell - 1981 - BERTLMANN'S SOCKS AND THE NATURE OF REALITY.pdf:application/pdf}
