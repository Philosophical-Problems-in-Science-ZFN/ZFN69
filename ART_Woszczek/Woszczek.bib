@article{aolita_fully_2012,
	title = {Fully nonlocal quantum correlations},
	volume = {85},
	url = {https://link.aps.org/doi/10.1103/PhysRevA.85.032107},
	doi = {10.1103/PhysRevA.85.032107},
	abstract = {Quantum mechanics is a nonlocal theory, but not as nonlocal as the no-signalling principle allows. However, there exist quantum correlations that exhibit maximal nonlocality: they are as nonlocal as any nonsignalling correlation and thus have a local content, quantified by the fraction pL of events admitting a local description, equal to zero. We exploit the known link between the Kochen-Specker and Bell theorems to derive a maximal violation of a Bell inequality from every Kochen-Specker proof. We then show that these Bell inequalities lead to experimental bounds on the local content of quantum correlations that are significantly better than those based on other constructions. We perform the experimental demonstration of a Bell test originating from the Peres-Mermin Kochen-Specker proof, providing an upper bound on the local content pL≲0.22.},
	language = {en},
	number = {3},
	urldate = {2020-11-24},
	journal = {Physical Review A},
	author = {Aolita, Leandro and Gallego, Rodrigo and Acín, Antonio and Chiuri, Andrea and Vallone, Giuseppe and Mataloni, Paolo and Cabello, Adán},
