@article{heunen_bohrification_2012,
	title = {Bohrification of operator algebras and quantum logic},
	volume = {186},
	issn = {1573-0964},
	url = {https://doi.org/10.1007/s11229-011-9918-4},
	doi = {10.1007/s11229-011-9918-4},
	abstract = {Following Birkhoff and von Neumann, quantum logic has traditionally been based on the lattice of closed linear subspaces of some Hilbert space, or, more generally, on the lattice of projections in a von Neumann algebra A. Unfortunately, the logical interpretation of these lattices is impaired by their nondistributivity and by various other problems. We show that a possible resolution of these difficulties, suggested by the ideas of Bohr, emerges if instead of single projections one considers elementary propositions to be families of projections indexed by a partially ordered set \$\$\{{\textbackslash}mathcal\{C\}(A)\}\$\$of appropriate commutative subalgebras of A. In fact, to achieve both maximal generality and ease of use within topos theory, we assume that A is a so-called Rickart C*-algebra and that \$\$\{{\textbackslash}mathcal\{C\}(A)\}\$\$consists of all unital commutative Rickart C*-subalgebras of A. Such families of projections form a Heyting algebra in a natural way, so that the associated propositional logic is intuitionistic: distributivity is recovered at the expense of the law of the excluded middle. Subsequently, generalizing an earlier computation for n × n matrices, we prove that the Heyting algebra thus associated to A arises as a basis for the internal Gelfand spectrum (in the sense of Banaschewski–Mulvey) of the “Bohrification” \$\$\{{\textbackslash}underline A\}\$\$of A, which is a commutative Rickart C*-algebra in the topos of functors from \$\$\{{\textbackslash}mathcal\{C\}A\}\$\$to the category of sets. We explain the relationship of this construction to partial Boolean algebras and Bruns–Lakser completions. Finally, we establish a connection between probability measures on the lattice of projections on a Hilbert space H and probability valuations on the internal Gelfand spectrum of \$\$\{{\textbackslash}underline A\}\$\$for A = B(H).},
	language = {en},
	number = {3},
	urldate = {2020-11-25},
	journal = {Synthese},
	author = {Heunen, Chris and Landsman, Nicolaas P. and Spitters, Bas},
	month = jun,
	year = {2012},
	pages = {719--752},
	file = {Springer Full Text PDF:/home/pawel/.zotero/zotero/2felkxqq.default/zotero/storage/558UPCIC/Heunen et al. - 2012 - Bohrification of operator algebras and quantum log.pdf:application/pdf}
}

@article{horodecki_axiomatic_2015,
	title = {Axiomatic approach to contextuality and nonlocality},
	volume = {92},
	url = {https://link.aps.org/doi/10.1103/PhysRevA.92.032104},
	doi = {10.1103/PhysRevA.92.032104},
	abstract = {We present a unified axiomatic approach to contextuality and nonlocality based on the fact that both are resource theories. In those theories, the main objects are consistent boxes, which can be transformed by certain operations to achieve certain tasks. The amount of resource is quantified by appropriate measures of the resource. Following a recent paper [J. I. de Vicente, J. Phys. A: Math. Theor. 47, 424017 (2014)], and recent development of abstract approach to resource theories, such as entanglement theory, we propose axioms and welcome properties for operations and measures of resources. As one of the axioms of the measure we propose the asymptotic continuity: the measure should not differ on boxes that are close to each other by more than the distance with a factor depending logarithmically on the dimension of the boxes. We prove that relative entropy of contextuality is asymptotically continuous. Considering another concept from entanglement theory (the convex roof of a measure), we prove that for some nonlocal and contextual polytopes, the relative entropy of a resource is upper bounded up to a constant factor by the cost of the resource. Finally, we prove that providing a measure X of resource does not increase under allowed class of operations such as, e.g., wirings, the maximal distillable resource which can be obtained by these operations is bounded from above by the value of X up to a constant factor. We show explicitly which axioms are used in the proofs of presented results, so that analogous results may remain true in other resource theories with analogous axioms. We also make use of the known distillation protocol of bipartite nonlocality to show how contextual resources can be distilled.},
	number = {3},
	urldate = {2020-11-25},
	journal = {Physical Review A},
	author = {Horodecki, Karol and Grudka, Andrzej and Joshi, Pankaj and Kłobus, Waldemar and Łodyga, Justyna},
	month = sep,
	year = {2015},
	note = {Publisher: American Physical Society},
	pages = {032104},
	file = {APS Snapshot:/home/pawel/.zotero/zotero/2felkxqq.default/zotero/storage/8XABEQ6E/PhysRevA.92.html:text/html;Przesłana wersja:/home/pawel/.zotero/zotero/2felkxqq.default/zotero/storage/HKC3SBLK/Horodecki et al. - 2015 - Axiomatic approach to contextuality and nonlocalit.pdf:application/pdf}
