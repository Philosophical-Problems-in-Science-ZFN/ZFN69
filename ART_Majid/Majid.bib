@article{AJL,
	title = {Dynamically triangulating {Lorentzian} quantum gravity},
	volume = {610},
	issn = {0550-3213},
	url = {http://www.sciencedirect.com/science/article/pii/S0550321301002978},
	doi = {10.1016/S0550-3213(01)00297-8},
	abstract = {Fruitful ideas on how to quantize gravity are few and far between. In this paper, we give a complete description of a recently introduced non-perturbative gravitational path integral whose continuum limit has already been investigated extensively in d{\textless}4, with promising results. It is based on a simplicial regularization of Lorentzian spacetimes and, most importantly, possesses a well-defined, non-perturbative Wick rotation. We present a detailed analysis of the geometric and mathematical properties of the discretized model in d=3,4. This includes a derivation of Lorentzian simplicial manifold constraints, the gravitational actions and their Wick rotation. We define a transfer matrix for the system and show that it leads to a well-defined self-adjoint Hamiltonian. In view of numerical simulations, we also suggest sets of Lorentzian Monte Carlo moves. We demonstrate that certain pathological phases found previously in Euclidean models of dynamical triangulations cannot be realized in the Lorentzian case.},
	language = {en},
	number = {1},
	urldate = {2020-11-20},
	journal = {Nuclear Physics B},
	author = {Ambjørn, J. and Jurkiewicz, J. and Loll, R.},
	month = sep,
	year = {2001},
	pages = {347--382},
	file = {ScienceDirect Full Text PDF:/home/pawel/.zotero/zotero/2felkxqq.default/zotero/storage/UL88PHGF/Ambjørn et al. - 2001 - Dynamically triangulating Lorentzian quantum gravi.pdf:application/pdf;ScienceDirect Snapshot:/home/pawel/.zotero/zotero/2felkxqq.default/zotero/storage/NDE7P9P4/S0550321301002978.html:text/html}
}

@article{ArgMa,
	title = {Quantum gravity on polygons and \${\textbackslash}{Bbb} {R}{\textbackslash}times {\textbackslash}{Bbb} {Z}\_n\$ {FLRW} model},
	url = {https://inspirehep.net/literature/1798360},
	abstract = {We fully solve the quantum geometry of \${\textbackslash}Bbb Z\_n\$ as a polygon graph with arbitrary metric lengths on the edges, finding a \$*\$-preserving quantum Levi-Civita connection which is unique for \$n{\textbackslash}ne 4\$. As a first application, we numerically compute correlation functions for Euclideanised quantum gravity on \${\textbackslash}Bbb Z\_n\$ for small \$n\$. We then study an FLRW model on \${\textbackslash}Bbb R{\textbackslash}times{\textbackslash}Bbb Z\_n\$, finding the same expansion rate as for the classical flat FLRW model in 1+2 dimensions. We also look at particle creation on \${\textbackslash}Bbb R{\textbackslash}times {\textbackslash}Bbb Z\_n\$ and find an additional \$m=0\$ adiabatic no particle creation expansion as well as the particle creation spectrum for a smoothed step expansion.},
	language = {en},
	urldate = {2020-11-20},
	author = {Argota-Quiroz, J. N. and Majid, S.},
	month = may,
	year = {2020},
	note = {Number: arXiv:2005.13999},
	file = {Snapshot:/home/pawel/.zotero/zotero/2felkxqq.default/zotero/storage/SNAR7HF9/1798360.html:text/html}
}

