@phdthesis{james-phd-1996,
	type = {{PhD} {Thesis}},
	title = {Closed {Set} {Logic} in {Categories}},
	url = {https://digital.library.adelaide.edu.au/dspace/handle/2440/18746},
	abstract = {This thesis investigates two related aspects of a dualisation program for the intuitionist logic in categories. The dualisation program has as its end the presentation of closed set logic in place of the usual open set logic found in association with toposes. The study is concerned especially with Brouwerian algebras in categories as the duals of the usual Heyting algebras. Defines the notion of a sheaf over the closed sets of a topological space. Investigates the sheaves for their algebric properties in relation to base space topologies.},
	urldate = {2020-12-03},
	school = {Department of Philosophy, The University of Adelaide},
	author = {James, William},
